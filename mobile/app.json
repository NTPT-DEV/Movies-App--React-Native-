{
  "expo": {
    "name": " ",
    "slug": "MoviesApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-new.png",
    "scheme": "movieApp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-new.png",
        "backgroundColor": "#ffffff" 
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.MoviesApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-new.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "651ed41e-fa0d-4661-855a-9a4dddd55045"
      }
    },
    "owner": "ntpt"
  }
}
